{% if this.isInLayoutMode() %}

    {% if this.isCustomCssEnabled() %}
        <style rel="stylesheet" type="text/css" media="screen" data-custom-css>
        {{ this.getCustomCssText() }}
        </style>
    {% else %}
        <script type="text/css" data-custom-css>
        {{ this.getCustomCssText() }}
        </script>
    {% endif %}
{% endif %}